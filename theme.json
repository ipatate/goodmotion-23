{
  "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
  "customTemplates": [],
  "settings": {
    "appearanceTools": false,
    "color": {
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "custom": true,
      "link": false,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "color": "#333333",
          "name": "Foreground",
          "slug": "foreground"
        },
        {
          "color": "#ffffff",
          "name": "Background",
          "slug": "background"
        },
        {
          "color": "#08556c",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#f9c907",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#d84702",
          "name": "Tertiary",
          "slug": "tertiary"
        }
      ]
    },
    "custom": {
      "sizes": {
        "content": "1024px"
      },
      "colors": {
        "body": "#f4f5f6"
      },
      "typography": {
        "line-height": {
          "small": "1.1rem",
          "base": "1.3rem",
          "large": "1.50rem",
          "xlarge": "1.70rem",
          "gigantic": "2.25rem",
          "huge": "2.6rem"
        }
      }
    },
    "layout": {
      "contentSize": "var(--wp--custom--sizes--content)",
      "wideSize": "1275px"
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "spacingScale": {
        "steps": 10
      },
      "spacingSizes": [
        {
          "size": "clamp(0.5rem, 0.8vw, 1.1rem)",
          "slug": "10",
          "name": "10"
        },
        {
          "size": "clamp(0.5rem, 1vw, 1.5rem)",
          "slug": "20",
          "name": "20"
        },
        {
          "size": "clamp(1.5rem, 5vw, 2rem)",
          "slug": "30",
          "name": "30"
        },
        {
          "size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
          "slug": "40",
          "name": "40"
        },
        {
          "size": "clamp(2.5rem, 8vw, 6.5rem)",
          "slug": "50",
          "name": "50"
        },
        {
          "size": "clamp(3.75rem, 10vw, 7rem)",
          "slug": "60",
          "name": "60"
        }
      ],
      "units": [
        "%",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "textTransform": true,
      "fontStyle": true,
      "customFontSize": true,
      "textDecoration": true,
      "lineHeight": true,
      "letterSpacing": true,
      "fontWeight": true,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Sora",
              "fontStretch": "25% 151%",
              "fontWeight": "100 800",
              "fontStyle": "oblique 0deg 10deg",
              "unicodeRange": "U+000-5FF;",
              "src": [
                "file:./assets/fonts/Sora-VariableFont_wght.woff2"
              ]
            }
          ],
          "fontFamily": "\"Sora\", Adjusted Arial, sans-serif",
          "name": "Sora",
          "slug": "Sora"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/Lora-Regular.woff2"
              ]
            },
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/Lora-Italic.woff2"
              ]
            },
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/Lora-BoldItalic.woff2"
              ]
            },
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/Lora-Bold.woff2"
              ]
            }
          ],
          "fontFamily": "\"Lora\", Adjusted Times New Roman, serif",
          "name": "Lora",
          "slug": "Lora"
        }
      ],
      "fluid": true,
      "fontSizes": [
        {
          "size": "3rem",
          "fluid": {
            "min": "2.6rem",
            "max": "3.3rem"
          },
          "slug": "huge",
          "name": "Huge"
        },
        {
          "size": "2.2rem",
          "fluid": {
            "min": "2rem",
            "max": "2.4rem"
          },
          "slug": "gigantic",
          "name": "Gigantic"
        },
        {
          "size": "1.6rem",
          "fluid": {
            "min": "1.5rem",
            "max": "1.875rem"
          },
          "slug": "2xlarge",
          "name": "2Xlarge"
        },
        {
          "size": "1.4rem",
          "fluid": {
            "min": "1.3rem",
            "max": "1.5rem"
          },
          "slug": "xlarge",
          "name": "Xlarge"
        },
        {
          "size": "1.125rem",
          "fluid": {
            "min": "1.1rem",
            "max": "1.125rem"
          },
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "1rem",
          "slug": "base",
          "name": "Base",
          "fluid": false
        },
        {
          "size": "0.875rem",
          "slug": "small",
          "name": "Small",
          "fluid": false
        },
        {
          "size": "0.75rem",
          "slug": "xsmall",
          "name": "Xsmall",
          "fluid": false
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/separator": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/button": {
        "border": {
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--background)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--base)"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/quote": {
        "border": {
          "width": "3px",
          "color": "var(--wp--preset--color--secondary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontStyle": "normal",
          "fontWeight": "normal",
          "lineHeight": "var(--wp--custom--typography--line-height--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp-preset--font-size--gigantic)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)",
            "bottom": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--2-xlarge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--xlarge)"
        },
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)",
            "bottom": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--1-xlarge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--xlarge)"
        },
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--xlarge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--large)"
        },
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--xlarge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--large)"
        },
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--tertiary)"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--30)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--lora)",
      "fontSize": "var(--wp--preset--font-size--large)",
      "lineHeight": "var(--wp--custom--typography--line-height--xlarge)"
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "version": 2
}
