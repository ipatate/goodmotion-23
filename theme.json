{
  "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
  "customTemplates": [],
  "settings": {
    "appearanceTools": true,
    "color": {
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "custom": true,
      "link": false,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "color": "#333333",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#ffffff",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#555555",
          "name": "Text",
          "slug": "text"
        },
        {
          "color": "#08556c",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#f9c907",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#d84702",
          "name": "Tertiary",
          "slug": "tertiary"
        },
        {
          "color": "#f3f3f3",
          "name": "gray-clear",
          "slug": "gray-clear"
        },
        {
          "color": "#f5f9fb",
          "name": "Freeze",
          "slug": "freeze"
        }
      ]
    },
    "custom": {
      "typography": {
        "line-height": {
          "small": "1.1rem",
          "medium": "1.3rem",
          "large": "1.50rem",
          "x-large": "1.70rem",
          "huge": "2.25rem",
          "gigantic": "2.6rem"
        }
      }
    },
    "layout": {
      "contentSize": "1024px",
      "wideSize": "1275px"
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "spacingScale": {
        "operator": "*",
        "increment": 2,
        "steps": 10,
        "mediumStep": 1.5,
        "unit": "rem"
      },
      "spacingSizes": [
        {
          "size": "clamp(0.5rem, 0.8vw, 1.1rem)",
          "slug": "10",
          "name": "10"
        },
        {
          "size": "clamp(0.5rem, 1vw, 1.5rem)",
          "slug": "20",
          "name": "20"
        },
        {
          "size": "clamp(1.5rem, 5vw, 2rem)",
          "slug": "30",
          "name": "30"
        },
        {
          "size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
          "slug": "40",
          "name": "40"
        },
        {
          "size": "clamp(2.5rem, 8vw, 6.5rem)",
          "slug": "50",
          "name": "50"
        },
        {
          "size": "clamp(3rem, 10vw, 7rem)",
          "slug": "60",
          "name": "60"
        }
      ],
      "units": [
        "%",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "textTransform": true,
      "fontStyle": true,
      "customFontSize": true,
      "textDecoration": true,
      "lineHeight": true,
      "letterSpacing": true,
      "fontWeight": true,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Sora",
              "fontStretch": "25% 151%",
              "fontWeight": "100 800",
              "fontStyle": "oblique 0deg 10deg",
              "unicodeRange": "U+000-5FF;",
              "src": [
                "file:./assets/fonts/Sora-VariableFont_wght.woff2"
              ]
            }
          ],
          "fontFamily": "\"Sora\", Arial, sans-serif",
          "name": "Sora",
          "slug": "Sora"
        }
      ],
      "fluid": true,
      "fontSizes": [
        {
          "size": "3rem",
          "fluid": {
            "min": "2.6rem",
            "max": "3.3rem"
          },
          "slug": "gigantic",
          "name": "Gigantic"
        },
        {
          "size": "2.2rem",
          "fluid": {
            "min": "2rem",
            "max": "2.4rem"
          },
          "slug": "huge",
          "name": "Huge"
        },
        {
          "size": "1.5rem",
          "fluid": {
            "min": "1.3rem",
            "max": "1.5rem"
          },
          "slug": "x-large",
          "name": "Xlarge"
        },
        {
          "size": "1.25rem",
          "fluid": {
            "min": "1.1rem",
            "max": "1.25rem"
          },
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "1rem",
          "slug": "medium",
          "name": "Medium",
          "fluid": false
        },
        {
          "size": "0.875rem",
          "slug": "small",
          "name": "Small",
          "fluid": false
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/separator": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/button": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)",
            "left": "var(--wp--preset--spacing--20)",
            "bottom": "var(--wp--preset--spacing--10)",
            "right": "var(--wp--preset--spacing--20)"
          }
        },
        "border": {
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--secondary)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "500"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--gigantic)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--huge)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "bottom": "var(--wp--preset--spacing--50)"
          }
        }
      },
      "core/pullquote": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)",
            "left": "var(--wp--preset--spacing--10)",
            "bottom": "var(--wp--preset--spacing--10)",
            "right": "var(--wp--preset--spacing--10)"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "border": {
          "width": "3px",
          "color": "var(--wp--preset--color--secondary)"
        }
      },
      "core/quote": {
        "border": {
          "width": "3px",
          "color": "var(--wp--preset--color--secondary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--gigantic)",
          "fontStyle": "normal",
          "fontWeight": "normal",
          "lineHeight": "var(--wp--custom--typography--line-height--gigantic)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/navigation": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontStyle": "normal",
          "fontWeight": "normal",
          "lineHeight": "var(--wp--custom--typography--line-height--large)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/navigation-link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--gray-clear)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--gigantic)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--gigantic)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)",
            "bottom": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)",
            "bottom": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--x-large)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--large)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--large)",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--text)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--tertiary)"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--10)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--sora)",
      "fontSize": "var(--wp--preset--font-size--large)",
      "lineHeight": "var(--wp--custom--typography--line-height--x-large)"
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "version": 2
}
